= form_for @account do |f|
  .form-group
    = f.label 'Account Name'
    = f.text_field :name, class: "form-control"
  .form-group
    = f.label 'Amount'
    = f.text_field :funds, class: "form-control"
  .form-group
    = f.submit class: "btn btn-default"
    = link_to 'Cancel', :back, class: "btn btn-default"
